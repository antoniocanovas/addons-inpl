<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    # Datos para rec√°lculo de tarifas:
    <record id="inplast_product_category_inh_form" model="ir.ui.view">
        <field name="name">inplast.product.category.inh.form</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="account.view_category_property_form"/>
        <field name="arch" type="xml">
            <data>
                <group name="first" position="after">
                    <group>
                        <group name="pricelist" string="Pricelist data">
                            <field name="pnt_material_type"/>
                            <field name="pnt_pricelist_weight"/>
                            <field name="pnt_mrp_fault_percent"/>
                        </group>
                    </group>
                </group>
            </data>
        </field>
        <field name="priority">99</field>
        <field name="type">form</field>
    </record>

</odoo>
